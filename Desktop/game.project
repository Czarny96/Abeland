[project]
title = Desktop
dependencies = https://github.com/britzl/defold-luasocket/archive/0.11.zip
write_log = 0

[bootstrap]
main_collection = /main/main.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1920
height = 1080
fullscreen = 0

[script]
shared_state = 1

[windows]
app_icon = /assets/app_icons/appicon.ico

[osx]
app_icon = /assets/app_icons/appicon.icns

[physics]
max_collisions = 512
max_contacts = 512

